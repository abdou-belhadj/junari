<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="new_stage" model="crops.stages">
            <field name="name">New</field>
            <field name="sequence">1</field>
            <field name="is_closing_opening" eval="True"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="plant_stage" model="crops.stages">
            <field name="name">Plant</field>
            <field name="sequence">10</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="germination_stage" model="crops.stages">
            <field name="name">Germination</field>
            <field name="sequence">20</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="seedling_stage" model="crops.stages">
            <field name="name">Seedling</field>
            <field name="sequence">20</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="vegetative_growth_stage" model="crops.stages">
            <field name="name">Vegetative growth</field>
            <field name="sequence">30</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="flowering_stage" model="crops.stages">
            <field name="name">Flowering</field>
            <field name="sequence">40</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="pollination_stage" model="crops.stages">
            <field name="name">Pollination</field>
            <field name="sequence">50</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="ripening_stage" model="crops.stages">
            <field name="name">Ripening</field>
            <field name="sequence">60</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="fruit_development_stage" model="crops.stages">
            <field name="name">Fruit development</field>
            <field name="sequence">70</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="harvesting_stage" model="crops.stages">
            <field name="name">Harvesting</field>
            <field name="sequence">80</field>
            <field name="is_closing_opening" eval="False"/>
            <field name="is_harvest" eval="True"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="done_stage" model="crops.stages">
            <field name="name">Done</field>
            <field name="sequence">500</field>
            <field name="is_closing_opening" eval="True"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="cancel_stage" model="crops.stages">
            <field name="name">Cancel</field>
            <field name="sequence">900</field>
            <field name="is_closing_opening" eval="True"/>
            <field name="fold" eval="True"/>
        </record>

    </data>
</odoo>